#include <bits/stdc++.h>
#include<cstdio>
using namespace std;

int update(int *a,int *b) {
    int m,n;
    m=*a+*b;
    n=abs(*a-*b);
    cout<<m<<endl<<n<<endl;
    return 0;
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;

    scanf("%d %d", &a, &b);
    update(pa, pb);
    //printf("%d\n%d", a, b);

    return 0;
}
